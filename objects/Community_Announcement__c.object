    <validationRules>
        <fullName>Limit_Ttitle_Chars_on_Copa_Update</fullName>
        <active>true</active>
        <description>This rule is used to limit the number of character to 58 on Message/Title field when creating Copa Update. As increasing the number of character in this field may makes the UI of Copa Update wierd.</description>
        <errorConditionFormula>IF( AND(LEN(Message__c) &gt; 58, RecordType.DeveloperName == &apos;Copa_Update&apos;), true, false)</errorConditionFormula>
