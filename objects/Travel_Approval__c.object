    <validationRules>
        <fullName>Estimated_Expenses_Required_For_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>ispickval (Status__c, &apos;Pending Approval&apos;) 
&amp;&amp;  (isblank(Total_Expenses__c ) || Total_Expenses__c = 0)</errorConditionFormula>
        <errorMessage>Please provide an estimate of Expenses.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Trip_end_date_after_start_date</fullName>
        <active>true</active>
        <errorConditionFormula>Trip_End_Date__c &lt; Trip_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Trip_End_Date__c</errorDisplayField>
        <errorMessage>Trip end date must be greater than or equal to start date</errorMessage>
    </validationRules>
