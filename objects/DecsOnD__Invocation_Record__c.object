    <validationRules>
        <fullName>DecsOnD__Prevent_changing_status</fullName>
        <active>true</active>
        <description>Used to stop Batch jobs - a race condition occurs and batch manages to log last execution entry.</description>
        <errorConditionFormula>(ISPICKVAL(PRIORVALUE(DecsOnD__Status__c	),&quot;Aborted&quot;) || 
ISPICKVAL(PRIORVALUE(DecsOnD__Status__c	),&quot;Completed&quot;) || 
ISPICKVAL(PRIORVALUE(DecsOnD__Status__c	),&quot;Finished&quot;))
&amp;&amp; ISPICKVAL(DecsOnD__Type__c, &quot;Scheduled-Batch&quot;)</errorConditionFormula>
        <errorMessage>Cannot change status once reached final status</errorMessage>
    </validationRules>
