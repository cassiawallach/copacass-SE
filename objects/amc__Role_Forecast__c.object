    <validationRules>
        <fullName>amc__End_Date_Validation</fullName>
        <active>true</active>
        <description>Ensures the End Date is not set before the Start Date</description>
        <errorConditionFormula>amc__Start_Date__c  &gt;  amc__End_Date__c</errorConditionFormula>
        <errorMessage>The End Date must be greater or equal to the Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Ensure_Working_Days_Greater_Than_Zero</fullName>
        <active>true</active>
        <description>Ensures the Working Days field is greater than 0</description>
        <errorConditionFormula>or(ISNULL(amc__Working_Days__c), amc__Working_Days__c  &lt;= 0)</errorConditionFormula>
        <errorMessage>The Working Days must be greater than 0</errorMessage>
    </validationRules>
