        <fullName>SLA_Business_Days__c</fullName>
        <externalId>false</externalId>
        <formula>(5 * ( FLOOR( ( SLA_Date_for_Attorney__c - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD(
SLA_Date_for_Attorney__c - DATE( 1900, 1, 8), 7 ) ) )
-
(5 * ( FLOOR( (  Today() - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD(
 Today() - DATE( 1900, 1, 8), 7 ) ) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
