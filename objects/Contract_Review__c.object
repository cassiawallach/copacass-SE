    <fields>
        <fullName>SLA_Date_for_Attorney__c</fullName>
        <businessStatus>Active</businessStatus>
        <externalId>false</externalId>
        <formula>CASE(
MOD(DATEVALUE(CreatedDate) - DATE(1900, 1, 7), 7), 
0, CreatedDate+15,
1, CreatedDate+13, 
2, CreatedDate+13,
3, CreatedDate+14, 
4, CreatedDate+14, 
5, CreatedDate+14, 
6, CreatedDate+14,
null
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>SLA Date for Attorney</label>
