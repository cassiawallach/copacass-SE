    <fields>
        <fullName>Location_Meeting_Link_For_Email__c</fullName>
        <description>This field is used in email template for copa event registration and it populates location or meeting link based on event venue.</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Venue__c, &quot;In-Person&quot;), Location__c, Meeting_Link__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Location/Meeting Link For Email</label>
        <required>false</required>
