    <validationRules>
        <fullName>End_Date_Cannot_Be_Prior_To_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c &gt; End_Date__c</errorConditionFormula>
        <errorMessage>End date should not be less then start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Registration_URL</fullName>
        <active>true</active>
        <description>Check whether the Registration URL starts with http or https</description>
        <errorConditionFormula>AND( 
    NOT(ISBLANK(Registration_Link__c)),
    NOT(BEGINS(LOWER(Registration_Link__c), &quot;https://&quot;)),
    NOT(BEGINS(LOWER(Registration_Link__c), &quot;http://&quot;))
)</errorConditionFormula>
        <errorMessage>Please provide complete URL including http or https.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Location_Required_For_In_Person_Events</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND(TEXT(Venue__c) == &apos;In-Person&apos;, ISBLANK( Location__c )), true, false)</errorConditionFormula>
        <errorMessage>Please provide the location for In-Person event.</errorMessage>
    </validationRules>
