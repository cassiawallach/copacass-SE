}

.ebsta-complex-add-record-icon.ebCAOtherResultIcon {
    background-position: -140px -20px;
}

.ebsta-complex-add-record-icon{
    width: 20px;
    height: 20px;
    background-image: url(https://secure1.ebsta-webservices.com/svg/BAR_20PX.svg);
    margin: auto;
    float: left;
}

.ebsta-complex-add-record-icon.ebCAAccountIcon{
    background-position: -40px 0px;
}

.ebsta-complex-add-record-icon.ebCAContactIcon{
    background-position: 0px 0px;
}

.ebsta-complex-add-record-icon.ebCALeadIcon{
    background-position: -20px 0px;
}

.ebsta-complex-add-record-icon.ebCAOpportunityIcon{
    background-position: -60px 0px;
}

.ebsta-complex-add-record-icon.ebCACaseIcon{
    background-position: -120px 0px;
}

.ebsta-complex-add-record-icon.ebCAOtherIcon{
    background-position: -140px 0px;
}

.ebsta-complex-add-search-input{
    border: none;
    color: #444444;
    display: block;
    width: 250px;
    outline: none;
    margin-top:2px;
    background: transparent;
}

.ebsta-complex-add-record-loader-gif{
	vertical-align: middle;
}

.ebsta-complex-add-chevron-down {
    background: url(https://secure1.ebsta-webservices.com/svg/glyphicons/glyphicons-halflings-114-chevron-down.svg);
    background-repeat: no-repeat;
    width: 25px;
    height: 25px;
    display: inline-block;
}

.ebsta-complex-add-chevron-right {
    background: url(https://secure1.ebsta-webservices.com/svg/glyphicons/glyphicons-halflings-80-chevron-right.svg);
    background-repeat: no-repeat;
    width: 25px;
    height: 25px;
    display: inline-block;
}

.ebsta-complex-add-trashicon {
    background: url(https://secure1.ebsta-webservices.com/svg/glyphicons/glyphicons-halflings-20-trash.svg);
    background-repeat: no-repeat;
    width: 25px;
    height: 25px;
    display: inline-block;
    cursor:pointer;
}

.ebsta-complex-add-removeicon {
    background: url(https://secure1.ebsta-webservices.com/svg/glyphicons/glyphicons-halflings-14-remove.svg);
    background-repeat: no-repeat;
    width: 25px;
    height: 25px;
    display: inline-block;
    cursor:pointer;
}

.ebsta-complex-add-magnifyicon {
    background: url(https://secure1.ebsta-webservices.com/svg/glyphicons/glyphicons-halflings-3-search.svg);
    background-repeat: no-repeat;
    width: 25px;
    height: 25px;
    display: inline-block;
    cursor:pointer;
}



.ebsta-complex-add-record-text{
	float: left;
    margin: 3px 0px 0px 3px;
    max-width: 200px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.ebsta-complex-add-record-add-remove-icon {
	width: 16px;
    height: 16px;
    float: right;
    display: inline-block;
    margin: 2px 2px 0px 5px;
    cursor: pointer;
    background-image: url(https://secure1.ebsta-webservices.com/svg/BAR_16PX.svg);
}

.ebsta-complex-add-record-add-remove-icon.ebCARemove {
    background-position: -48px 0px;
}

.ebsta-complex-add-record-add-remove-icon.ebCAAdd  {
    background-position: -32px 0px;
}

.ebsta-complex-add-save-button{
	float: right;
	margin-top: 15px;
}

.ebsta-complex-add-response {
    position: fixed;
    z-index: 10;
    top: 12%;
    left: 50%;
    transform: translate(-50%, -49%);
}

