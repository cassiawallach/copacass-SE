/** Complex Add **/

#ebsta-complex-add{
    position: fixed;
    top: 7%;
    left: 25%;
    color: #000;
    padding: 13px 25px 13px 45px;
    -webkit-box-shadow: 0 4px 16px rgba(0,0,0,.2);
    box-shadow: 0 0px 12px rgba(0,0,0,.2);
    background: #F5F5F5;
    border: 1px solid #acacac;
    max-height: 700px;
    margin-bottom: 150px;
    width: 715px;
    z-index: 100001!important;
}

#ebsta-complex-add h3{
	max-width: 600px;
}

#ebsta-complex-add div::-webkit-scrollbar, .ebsta-attachmentlist-ul::-webkit-scrollbar {
    width: 4px!important;
    height:9px!important;
}
 
#ebsta-complex-add div::-webkit-scrollbar-track, .ebsta-attachmentlist-ul::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3)!important; 
    border-radius: 2px!important;
    border: none!important;
}
 
#ebsta-complex-add div::-webkit-scrollbar-thumb, .ebsta-attachmentlist-ul::-webkit-scrollbar-thumb {
    border-radius: 2px!important;
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.5)!important; 
    border: none!important;
}

.ebsta-complex-add-row-container{
	max-height: 240px;
    overflow-y: auto;
    overflow-x: hidden;
}

.ebsta-complex-add-row {
    margin: 5px 0px 0px 0px;
    background: #FFF;
    padding: 5px;
    border-radius: 5px;
    border: 1px solid #acacac;
    width: 652px;
}

.ebsta-complex-add-row > div {
    float: left;
}

.ebsta-complex-add-row .ebsta-complex-add-chevron-right, .ebsta-complex-add-row .ebsta-complex-add-trashicon{
    margin-top: 4px
}

.ebsta-complex-add-segment{
	width: 295px;
}

.ebsta-complex-add-container{
    display: inline-block;
    border: 1px solid #ccc;
    padding: 5px;
    border-radius: 5px;
    position: relative;
    width: 290px;
    margin-right: 5px;
    height: 34px;
}

.ebsta-complex-add-container.disabled{
	background-color: #EBEBE4;
}

.ebsta-complex-add-results-container, .ebsta-complex-add-related-results-container{
	max-height: 90px;
    overflow: auto;
    margin: 5px 5px 0 0;
}

.ebsta-complex-add-related-results-container > span{
	display:block;
}

.ebsta-complex-add-results-container{
	max-height: 90px;
    overflow: auto;
}

.ebsta-complex-add-what-container{
	margin: 0;
}

.ebsta-complex-add-search-container > .ebsta-complex-add-magnifyicon{
    position: absolute;
    right: 0;
    top: 3px;
}

.ebsta-complex-add-container.disabled  .ebsta-complex-add-magnifyicon{
	display: none;
}

.ebsta-complex-add-record-container{
    display: inline-block;
    float: left;
    color: #ffffff!important;
    border-radius: 3px!important;
    padding: 1px 5px;
    width: auto;
    cursor: pointer;
}

.ebsta-complex-add-record-container.ebCAAccount{
   	background-color : #7f8de1;
}

.ebsta-complex-add-record-container.ebCAAccount:hover{
    background-color : #7481D3;
}

.ebsta-complex-add-record-container.ebCAContact{
   	background-color : #B36FAA;
}

.ebsta-complex-add-record-container.ebCAContact:hover{
    background-color : #BD77B4;
}

.ebsta-complex-add-record-container.ebCALead{
   	background-color : #F07554;
}

.ebsta-complex-add-record-container.ebCALead:hover{
    background-color : #E66948;
}

.ebsta-complex-add-record-container.ebCAOpportunity{
   	background-color : #f2ae4d;
}

.ebsta-complex-add-record-container.ebCAOpportunity:hover{
    background-color : #F3B65F;
}

.ebsta-complex-add-record-container.ebCACase{
   	background-color : #efc550;
}

.ebsta-complex-add-record-container.ebCACase:hover{
    background-color : #F5CE63;
}

.ebsta-complex-add-record-container.ebCAOther{
   	background-color : #D96383;
}

.ebsta-complex-add-record-container.ebCAOther:hover{
    background-color : #D96383;
}

.ebsta-complex-add-record-container.result{
    background-color: #EDEDED;
    border: 1px solid #B6B6B6;
    color: #444040!important;
    padding: 3px;
    margin: 0 3px 3px 0;
}

.ebsta-complex-add-record-container.result:hover{
    background-color: #D3D2D2;
}

.ebsta-complex-add-record-icon.ebCAContactResultIcon {
    background-position: 0px -20px;
}

.ebsta-complex-add-record-icon.ebCALeadResultIcon {
